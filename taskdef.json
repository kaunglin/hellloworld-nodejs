{
    "family": "simple-node-app",
    "taskRoleArn": "arn:aws:iam::176991310202:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::176991310202:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "containerDefinitions": [
      {
        "name": "simple-node-app",
        "image": "176991310202.dkr.ecr.us-east-1.amazonaws.com/simple-node-app:latest",
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ],
        "environment": [
          {
            "name": "NODE_ENV",
            "value": "production"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/simple-node-app",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "requiresCompatibilities": [
      "EC2"
    ],
    "cpu": "256",
    "memory": "512"
  }